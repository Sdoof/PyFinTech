Cell[CellGroupData[{

Cell["4.3 The Black-Scholes Equation and the Diffusion Equation", "Section",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}}],

Cell["\<\
We return to the standard and more general form of our partial \
differential equation:\
\>", "Text",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}}],

Cell[BoxData[
    FormBox[
      RowBox[{
        RowBox[{
          FractionBox[\(\[PartialD]V\), \(\[PartialD]t\),
            MultilineFunction->None], "+", 
          RowBox[{"S", " ", \((r - q)\), " ", 
            FractionBox[\(\[PartialD]V\), \(\[PartialD]S\),
              MultilineFunction->None]}], "+", 
          RowBox[{\(1\/2\), " ", \(\[Sigma]\^2\), " ", \(S\^2\), " ", 
            FractionBox[\(\[PartialD]\^2 V\), \(\[PartialD]S\^2\),
              MultilineFunction->None]}], "-", \(r\ V\)}], "=", " ", "0"}], 
      TraditionalForm]], "NumberedEquation",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}}],

Cell[TextData[{
