Cell[CellGroupData[{

Cell["Calculation of Short-Term Discount Factors", "Subsection",
  CellMargins->{{6, Inherited}, {Inherited, Inherited}}],

Cell["\<\
The discount factors associated with the short-term LIBOR data are \
calculated using the formulae

(a) if input data are continuously compounded,\
\>", "Text",
  CellMargins->{{6, Inherited}, {Inherited, Inherited}}],

Cell[BoxData[
    \(TraditionalForm\`{t, 
        r} \[Rule] {t, \[ExponentialE]\^\(1\/100\ \((\(-r\)\ t)\)\)}\)], 
  "NumberedEquation",
  CellMargins->{{6, Inherited}, {Inherited, Inherited}}],

Cell["(b) if input data are annually compounded,", "Text",
  CellMargins->{{6, Inherited}, {Inherited, Inherited}}],

Cell[BoxData[
    \(TraditionalForm\`{t, r} \[Rule] {t, 1\/\((1 + r\/100)\)\^t}\)], 
  "NumberedEquation",
  CellMargins->{{6, Inherited}, {Inherited, Inherited}}],

Cell[TextData[{
  "A ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " implementation of this mapping on pairs of values is"
}], "Text",
  CellMargins->{{6, Inherited}, {Inherited, Inherited}}],

Cell[BoxData[
    \(LiborDisc[libordata_List, \ compounding_String]\  := \n\t
      Which[\n\t\tcompounding == 
          "\<cc\>", \n\t\tMap[{#[\([1]\)], \ 
              Exp[\(-#[\([1]\)]\)*#[\([2]\)]/100]} &, \ 
          libordata], \n\t\t\tcompounding == 
          "\<an\>", \n\t\tMap[{#[\([1]\)], \ 
              1/\((1 + #[\([1]\)]/100)\)^#[\([2]\)]} &, \ 
          libordata], \n\t\tTrue, \ "\<This compounding not supported\>"]\)], 
  "Input",
  CellMargins->{{6, Inherited}, {Inherited, Inherited}}],

