Cell[CellGroupData[{

Cell["15.5 Fully Implicit Scheme for Put", "Section",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}},
  Evaluatable->False,
  AspectRatioFixed->True],

Cell["\<\
These algorithms are by now self-explanatory - first the \
initialization:\
\>", "Text",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}}],

Cell[BoxData[
    \(dx = 0.025; \ dtau = 0.00025; \ alpha = dtau\/dx\^2; \nM = 400; \ 
    nminus = 160; \ nplus = 160; \n\n
    initial = 
      Table[PutExercise[\((k - 1 - nminus)\)\ dx, 0.05, 0, 0.2], \ {k, 
          nminus + nplus + 1}]; \n
    lower = Table[
        f[\(-nminus\)\ dx, \((m - 1)\)\ dtau, 0.05, 0, 0.2], \ {m, \ 1, \ 
          M + 1}]; \n
    upper = Table[
        g[\(+nplus\)\ dx, \ \ \((m - 1)\)\ dtau, 0.05, 0, 0.2], \ {m, \ 1, \ 
          M + 1}]; \nwold\  = \ initial; \nwvold\  = \ wold; \ 
    wnew\  = \ wold; \n\n
    CMat\  = \ FullyImpCMatrix[alpha, nminus, nplus]; \)], "Input",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}},
  PageBreakAbove->False,
  GroupPageBreakWithin->True,
  AspectRatioFixed->True],

Cell["Evolving the solution (this may take some time):", "Text",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}}],

Cell["\<\
For[m=2, m<=M+1, m++,
(wvold = wold;
wold = wnew;
rhs = Take[wold, {2, -2}]+
Table[
Which[
k==1, alpha*lower[[m]],
k== nplus + nminus-1,  alpha*upper[[m]],
True, 0],
{k, 1, nplus + nminus-1}];
temp = CompTridiagSolve[CMat, rhs];
wnew = Join[{lower[[m]]}, temp, {upper[[m]]}])
]\
\>", "Input",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}},
  AspectRatioFixed->True],

Cell["Interpolation and construction of valuation function:", "Text",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}}],

Cell["\<\
interpoldata = Table[{(k - nminus - 1)*dx, wnew[[k]]}, 
    {k, 1, nminus + nplus + 1}]; \
\>", "Input",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}},
  AspectRatioFixed->True],

Cell["\<\
ufuncb = Interpolation[interpoldata, InterpolationOrder -> 3];\
\>",
   "Input",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}},
  AspectRatioFixed->True],

Cell["\<\
Valuation[strike_, r_, q_, S_, T_, sd_] := 
  ValuationMultiplier[strike, r, q, Log[S/strike], (sd^2*T)/2, sd]*
   ufuncb[Log[S/strike]]\
\>", "Input",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}},
  AspectRatioFixed->True],

