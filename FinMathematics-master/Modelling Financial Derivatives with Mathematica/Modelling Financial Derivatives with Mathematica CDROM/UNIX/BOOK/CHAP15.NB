Cell[BoxData[
    \(\(ExplicitSolver\  = \n\t
      Compile[{{initial, _Real, 1}, \ {lower, _Real, 1}, \ {upper, _Real, 1}, 
          alpha}, \n\t\tModule[{wold = initial, \ wnew = initial, \ wvold, m, 
            k, \ tsize\  = \ Length[lower], 
            xsize = Length[initial]}, \n\t\t\tFor[m = 2, \ 
            m\  <= \ tsize, \ \(m++\), \n\t\t\t\t\((wvold\  = \ wold; \ 
              wold = wnew; \n\t\t\t\t\tFor[k = 2, \ 
                k\  < \ xsize, \ \(k++\), \n\t\t\t\t\t\((wnew[\([k]\)]\  = \ 
                    alpha\ \((wold[\([k - 1]\)] + 
                            wold[\([k + 1]\)])\) + \((1 - 
                            2  alpha)\)\ wold[\([k]\)])\)]; \n\t\t\t\t\t\t\
wnew[\([1]\)]\  = \ lower[\([m]\)]; \n\t\t\t\t\t\twnew[\([xsize]\)]\  = \ 
                upper[\([m]\)])\)]; \n\t\t\t{wvold, wold, 
            wnew}]\n\t\t]; \)\)], "Input",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}},
  PageBreakWithin->True],

Cell[BoxData[
    \(\(CompTridiagSolve\  = \ \t\t\tCompile[{{a, \ _Real, \ 1}, {b, _Real, \ 
            1}, {c, \ _Real, \ 1}, {r, \ _Real, \ 
            1}}, \n\ \ \ \ Module[{len = Length[r], \ solution = r, \ 
            aux\  = \ 1/\((b[\([1]\)])\), \ aux1 = r, \ 
            a1\  = \ Prepend[a, \ 0.0], \ iter}, \ \tsolution[\([1]\)]\  = \ 
            aux\ *r[\([1]\)]; \[IndentingNewLine]Do[
            aux1[\([iter]\)]\  = \ 
              c[\([iter - 1]\)]\ aux; \n\t\t\t\ \ aux\  = \ 
              1/\((b[\([iter]\)]\  - \ 
                    a1[\([iter]\)]*
                      aux1[\([iter]\)])\); \n\t\t\t\tsolution[\([iter]\)]\  = \
\ \((r[\([iter]\)] - 
                    a1[\([iter]\)]\ solution[\([iter - 
                            1]\)])\)\ aux, \n\t\ \ \ \ {iter, \ 2, \ 
              len}]; \ \t\t\t\t\[IndentingNewLine]Do[
            solution[\([iter]\)]\  -= \ 
              aux1[\([iter + 1]\)]\ solution[\([iter + 
                      1]\)], \n\t\ \ \ \ {iter, \ len - 1, \ 
              1, \ \(-1\)}]; \[IndentingNewLine]\t\ \ \ \ solution]]; \)\)], 
  "Input",
  CellMargins->{{5, Inherited}, {Inherited, Inherited}}],

Cell["\<\
